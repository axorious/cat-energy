{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/catalog",
      "dest": "/catalog.html"
    },
    {
      "src": "/form",
      "dest": "/form.html"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    }
  ]
}
